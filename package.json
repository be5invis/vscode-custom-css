{
	"name": "clickable-button-hints",
	"displayName": "Clickable Button Hints",
	"description": "Clickable button hints for Visual Studio Code",
	"version": "1.0.0",
	"publisher": "tusharsadhwani",
	"author": {
		"email": "tushar.sadhwani000@gmail.com",
		"name": "Tushar Sadhwani",
		"url": "https://tusharsadhwani.dev"
	},
	"engines": {
		"vscode": "^1.28.0"
	},
	"categories": [
		"Other",
		"Themes"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/tusharsadhwani/clickable-button-hints"
	},
	"bugs": {
		"url": "https://github.com/tusharsadhwani/clickable-button-hints/issues"
	},
	"preview": true,
	"icon": "images/logo.png",
	"galleryBanner": {
		"color": "#EEEEEE"
	},
	"activationEvents": [
		"*"
	],
	"main": "./src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.installClickableButtonHints",
				"title": "Enable clickable button hints"
			},
			{
				"command": "extension.uninstallClickableButtonHints",
				"title": "Disable clickable button hints"
			},
			{
				"command": "extension.updateClickableButtonHints",
				"title": "Reload clickable button hints"
			}
		]
	},
	"devDependencies": {
		"eslint": "^7.21.0",
		"eslint-config-prettier": "^8.1.0"
	},
	"dependencies": {
		"file-url": "^3.0.0",
		"node-fetch": "^2.6.1",
		"uuid": "^8.3.2"
	},
	"extensionKind": [
		"ui"
	]
}
